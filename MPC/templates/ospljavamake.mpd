	cd $(CLASSDIR) <%and%> $(JAR) $(JARFLAGS) <%libgenopt%>$(JARFILE) $(MANIFEST) $(JARCLASSES)<%if(tempinc)%> `find $(TEMPINCDIR) -name \*.class\*`<%endif%> <%if(extra_ar_objs)%><%extra_ar_objs%><%endif%>
	$(MV) $(CLASSDIR)<%slash%><%libgenopt%>$(JARFILE) .
<%if(ranlib)%>
	ranlib $(JARFILE)
<%endif%>

$(CLASSDIR)$(MANIFEST):
