MAKEFILE      = <%project_file%>
DEPENDENCIES  = <%if(supports_include)%>.depend.<%endif%>$(MAKEFILE)
LTARGETDIR    = <%libout%><%if(!compares(libout, .))%><%output_dir_ext%><%endif%><%slash%><%targetoutdir%>
CAT           = <%type("cat")%>
MV            = <%move("mv -f")%>
RM            = <%delete("rm -rf")%>
