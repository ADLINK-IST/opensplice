<%endif%>
<%if(do_a_jar)%>
	-$(RM) $(CLASSDIR)$(MANIFEST)
<%endif%>
<%if(obj_dir)%>
	-$(RM) <%targetoutdir%><%obj_dir%>
<%endif%>
<%if(postclean)%>
	-<%eval(postclean)%>
<%endif%>

