. $OSPL_HOME/examples/functions

check_osplhome

get_libjsig

startOSPL

echo "=== Launching WaitSet "

LD_PRELOAD=$OSPL_JSIG $SPLICE_JAVA -classpath $OSPL_HOME/jar/dcpssaj5.jar:classes:$SPLICE_EXTRA_CP WaitSetDataSubscriber > subResult.txt &

sleep 5

LD_PRELOAD=$OSPL_JSIG $SPLICE_JAVA -classpath $OSPL_HOME/jar/dcpssaj5.jar:classes:$SPLICE_EXTRA_CP WaitSetDataPublisher > pubResult.txt

sleep 10

waitsetCheckResult

stopOSPL
