$sp($(spaces))      in DDS::ViewStateMask view_states,
$sp($(spaces))      in DDS::InstanceStateMask instance_states);

$sp($(spaces))    DDS::ReturnCode_t read_next_instance_w_condition(
$sp($(spaces))      inout $(typename)Seq received_data,
