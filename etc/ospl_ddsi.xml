<OpenSplice>
    <Domain>
        <Name>OpenSplice-DDSi__RELEASE__</Name>
        <Database>
            <Size>10485760</Size>
        </Database>
        <Lease>
            <ExpiryTime update_factor="0.05">60.0</ExpiryTime>
        </Lease>
        <Service name="ddsi">
            <Command>ddsi__EXE__</Command>
        </Service>
    </Domain>

    <DDSIService name="ddsi">
        <Channels>
            <Channel name="Channel-0" default="true" enabled="true">
                <PortNr>53370</PortNr>
            </Channel>
        </Channels>
         <Discovery enabled="true">
            <PortNr>7400</PortNr>
        </Discovery>
        <Partitioning>
            <GlobalPartition Address="239.255.0.1" />
        </Partitioning>
    </DDSIService>

</OpenSplice>
