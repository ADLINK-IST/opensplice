. "$OSPL_HOME/bin/checkconf"

SPLICE_HOST=armv8.linux-dev
SPLICE_TARGET=armv8.linux-dev
export SPLICE_HOST SPLICE_TARGET
SPLICE_TARGET_FULL=armv8.linux
export SPLICE_TARGET_FULL
echo

ERRS=0
gcc_check || ERRS=1
make_check || ERRS=1
gawk_check || ERRS=1
bison_check || ERRS=1
flex_check || ERRS=1
gsoap_check || ERRS=1
javac_check || ERRS=1
gmcs_check || ERRS=1
tao_check || ERRS=1
jacorb_check || ERRS=1
protoc_check || ERRS=1
c99_check || ERRS=1
python3_check || ERRS=1
maven_check_inner || ERRS=1

if [ -z $REPORT ]
then
    if [ "$ERRS" = "0" ]
    then
        echo Configuration OK
        CONFIGURATION=OK
    else
        echo Configuration Invalid
        CONFIGURATION=INVALID
    fi
    export CONFIGURATION
    cleanup_checks
fi
