. $OSPL_HOME/bin/checkconf

SPLICE_HOST=sparc.solaris10_studio12-release
SPLICE_TARGET=sparc.solaris10_studio12-release
export SPLICE_HOST SPLICE_TARGET

ERRS=0

binutils_native_check || ERRS=1
sun_cc_studio12_check || ERRS=1
make_check || ERRS=1
gawk_check || ERRS=1
bison_check || ERRS=1
flex_check || ERRS=1
javac_check || ERRS=1
gmcs_check || ERRS=1
tao_check || ERRS=1
jacorb_check || ERRS=1

if [ -z "$REPORT" ]
then
    if [ "$ERRS" = "0" ]
    then
        echo Configuration OK
        CONFIGURATION=OK
    else
        echo Configuration Invalid
        CONFIGURATION=INVALID
    fi
    export CONFIGURATION
    cleanup_checks
fi
