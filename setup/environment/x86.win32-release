. "$OSPL_HOME/bin/checkconf"

SPLICE_HOST=x86.win32-release
SPLICE_TARGET=x86.win32-release
export SPLICE_HOST SPLICE_TARGET
echo

ERRS=0
msvs_check || ERRS=1
csharp_check || ERRS=1
make_check || ERRS=1
gawk_check || ERRS=1
bison_check || ERRS=1
flex_check || ERRS=1
javac_check || ERRS=1
tao_check || ERRS=1
jacorb_check || ERRS=1
gsoap_check || ERRS=1
doxygen_check || ERRS=1
protoc_check || ERRS=1
c99_check || ERRS=1
python3_check || ERRS=1
maven_check_inner || ERRS=1
key_value_store_check || ERRS=1

export SPLICE_HOST_FULL=x86.win-vs${VS_YEAR}

if [ -z $REPORT ]
then
    if [ "$ERRS" = "0" ]
    then
        echo Configuration OK
        CONFIGURATION=OK
    else
        echo Configuration Invalid
        CONFIGURATION=INVALID
    fi
    export CONFIGURATION
    cleanup_checks
fi
