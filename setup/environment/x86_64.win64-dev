. $OSPL_HOME/bin/checkconf

SPLICE_HOST=x86_64.win64-dev
SPLICE_TARGET=x86_64.win64-dev
export SPLICE_HOST SPLICE_TARGET
echo

ERRS=0
msvs_check || ERRS=1
csharp_check || ERRS=1
make_check || ERRS=1
gawk_check || ERRS=1
bison_check || ERRS=1
flex_check || ERRS=1
javac_check || ERRS=1
tao_check || ERRS=1
jacorb_check || ERRS=1
gsoap_check || ERRS=1
doxygen_check || ERRS=1
protoc_check || ERRS=1
c99_check || ERRS=1

export SPLICE_HOST_FULL=x86_64.win-vs${VS_YEAR}-dev

if [ -z $REPORT ]
then
    if [ "$ERRS" = "0" ]
    then
        echo Configuration OK
        CONFIGURATION=OK
    else
        echo Configuration Invalid
        CONFIGURATION=INVALID
    fi
    export CONFIGURATION
    cleanup_checks
fi
