all:		link
link:		$(addsuffix .ss_link,  $(SUBSYSTEMS) $(PSUBSYSTEMS))
clean:		$(addsuffix .ss_clean, $(SUBSYSTEMS) $(PSUBSYSTEMS))

%.ss_link:	; @$(MAKE) -C $* link
%.ss_clean:	; @$(MAKE) -C $* clean

.PHONY: %.ss_link %.ss_clean
.PHONY: PRESUBSYSTEMS

